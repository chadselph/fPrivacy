{
    "name": "fPrivacy",
    "version": "0.3.0",
    "description": "Makes Facebook permissions optional.",
    "permissions": [
        "https://www.facebook.com"
    ],
    "content_scripts": [{
        "matches": ["*://*.facebook.com/dialog/permissions.request*","*://*.facebook.com/connect/uiserver*","*://*.facebook.com/dialog/oauth*"],
        "js": ["extension.js"],
        "css": ["style.css"]
    }],
    "icons": {
        "16": "icons/icon16.png",
        "48":  "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
